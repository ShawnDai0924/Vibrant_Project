{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { Loader } from '@googlemaps/js-api-loader';\nexport default {\n  props: {\n    selectedState: {\n      type: String,\n      required: false\n    }\n  },\n  setup(props) {\n    const map = ref(null);\n    const loader = new Loader({\n      apiKey: 'AIzaSyB1-psa7VQjYCGqGVYsLJVrOfdnYOYQuFs',\n      version: 'weekly'\n    });\n    onMounted(() => {\n      loader.load().then(() => {\n        const mapOptions = {\n          center: {\n            lat: 37.1,\n            lng: -95.7\n          },\n          // 美国中部\n          zoom: 4\n        };\n        map.value = new google.maps.Map(document.getElementById('map'), mapOptions);\n        highlightState(props.selectedState); // 高亮选中的州\n      });\n    });\n    // 高亮显示选中的州（您可以根据州的名称调整位置）\n    function highlightState(stateName) {\n      if (!stateName) return;\n      // 这里可以使用不同的多边形坐标来高亮显示不同的州\n      const stateCoordinates = {\n        'Alabama': {\n          paths: [/* Alabama coordinates */],\n          options: {\n            fillColor: 'yellow'\n          }\n        }\n        // 为每个州添加坐标...\n      };\n      if (stateCoordinates[stateName]) {\n        new google.maps.Polygon({\n          ...stateCoordinates[stateName].options,\n          paths: stateCoordinates[stateName].paths,\n          map: map.value\n        });\n      }\n    }\n    return {\n      map\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","Loader","props","selectedState","type","String","required","setup","map","loader","apiKey","version","load","then","mapOptions","center","lat","lng","zoom","value","google","maps","Map","document","getElementById","highlightState","stateName","stateCoordinates","paths","options","fillColor","Polygon"],"sources":["D:\\state_search_demo\\state-search-app\\src\\components\\MapView.vue"],"sourcesContent":["<template>\r\n    <div id=\"map\" style=\"height: 500px;\"></div>\r\n  </template>\r\n   <script>\r\n  import { ref, onMounted } from 'vue';\r\n  import { Loader } from '@googlemaps/js-api-loader';\r\n   export default {\r\n    props: {\r\n      selectedState: {\r\n        type: String,\r\n        required: false,\r\n      },\r\n    },\r\n    setup(props) {\r\n      const map = ref(null);\r\n      const loader = new Loader({\r\n        apiKey: 'AIzaSyB1-psa7VQjYCGqGVYsLJVrOfdnYOYQuFs', \r\n        version: 'weekly',\r\n      });\r\n       onMounted(() => {\r\n        loader.load().then(() => {\r\n          const mapOptions = {\r\n            center: { lat: 37.1, lng: -95.7 }, // 美国中部\r\n            zoom: 4,\r\n          };\r\n          map.value = new google.maps.Map(document.getElementById('map'), mapOptions);\r\n          highlightState(props.selectedState); // 高亮选中的州\r\n        });\r\n      });\r\n       // 高亮显示选中的州（您可以根据州的名称调整位置）\r\n      function highlightState(stateName) {\r\n        if (!stateName) return;\r\n         // 这里可以使用不同的多边形坐标来高亮显示不同的州\r\n        const stateCoordinates = {\r\n          'Alabama': { paths: [/* Alabama coordinates */], options: { fillColor: 'yellow' } },\r\n          // 为每个州添加坐标...\r\n        };\r\n         if (stateCoordinates[stateName]) {\r\n          new google.maps.Polygon({\r\n            ...stateCoordinates[stateName].options,\r\n            paths: stateCoordinates[stateName].paths,\r\n            map: map.value,\r\n          });\r\n        }\r\n      }\r\n       return {\r\n        map,\r\n      };\r\n    },\r\n  };\r\n  </script>\r\n\r\n  \r\n "],"mappings":"AAIE,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,MAAK,QAAS,2BAA2B;AACjD,eAAe;EACdC,KAAK,EAAE;IACLC,aAAa,EAAE;MACbC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAKA,CAACL,KAAK,EAAE;IACX,MAAMM,GAAE,GAAIT,GAAG,CAAC,IAAI,CAAC;IACrB,MAAMU,MAAK,GAAI,IAAIR,MAAM,CAAC;MACxBS,MAAM,EAAE,yCAAyC;MACjDC,OAAO,EAAE;IACX,CAAC,CAAC;IACDX,SAAS,CAAC,MAAM;MACfS,MAAM,CAACG,IAAI,CAAC,CAAC,CAACC,IAAI,CAAC,MAAM;QACvB,MAAMC,UAAS,GAAI;UACjBC,MAAM,EAAE;YAAEC,GAAG,EAAE,IAAI;YAAEC,GAAG,EAAE,CAAC;UAAK,CAAC;UAAE;UACnCC,IAAI,EAAE;QA